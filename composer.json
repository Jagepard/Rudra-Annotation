{
    "name": "rudra/annotation",
    "description": "Class & Method Annotation Reader",
    "type": "library",
    "keywords": [
        "rudra",
        "annotations",
        "annotation-processing",
        "annotation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jagepard",
            "email": "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru", 
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jagepard@yandex.ru",
        "issues": "https://github.com/Jagepard/Rudra-Annotation/issues", 
        "source": "https://github.com/Jagepard/Rudra-Annotation" 
    },
    "require": {
        "php": ">=8.3",
        "rudra/docs": "v25.10",
        "rudra/exception": "v25.10"
    },
    "autoload": {
        "psr-4": {
            "Rudra\\Annotation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rudra\\Annotation\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
